<plugin>
    <groupId>org.openrewrite.maven</groupId>
    <artifactId>rewrite-maven-plugin</artifactId>
    <version>5.23.1</version>
    <configuration>
        <activeRecipes>
            <!--Check for new methods on JDBC interfaces-->
            <recipe>org.openrewrite.java.migrate.JREJdbcInterfaceNewMethods</recipe>
            <!--Use the default InitialContext JNDI properties-->
            <recipe>org.openrewrite.java.liberty.RemoveWas2LibertyNonPortableJndiLookup</recipe>
            <!--MBean and MXBean interfaces must be public-->
            <recipe>org.openrewrite.java.migrate.MXBeanRule</recipe>
        </activeRecipes>
    </configuration>
    <dependencies>
        <dependency>
            <groupId>org.openrewrite.recipe</groupId>
            <artifactId>rewrite-migrate-java</artifactId>
            <version>2.9.1</version>
        </dependency>
        <dependency>
            <groupId>org.openrewrite.recipe</groupId>
            <artifactId>rewrite-liberty</artifactId>
            <version>1.1.3</version>
        </dependency>
    </dependencies>
</plugin>
